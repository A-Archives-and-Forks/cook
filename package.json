    "@pinia/testing": "^1.0.2",
    "@tauri-apps/cli": "^2.7.1",
    "@unhead/vue": "^2.0.14",
    "@unocss/eslint-config": "^66.4.2",
    "@unocss/nuxt": "^66.4.2",
    "@vite-pwa/nuxt": "^1.0.4",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/nuxt": "^13.6.0",
    "@yunlefun/vue": "^0.1.1",
    "bumpp": "^10.2.3",
    "consola": "^3.4.2",
    "dexie": "^4.0.11",
    "eslint": "^9.33.0",
    "eslint-plugin-format": "^1.0.1",
    "fake-indexeddb": "^6.1.0",
    "jsdom": "^26.1.0",
    "lint-staged": "^16.1.5",
    "nuxt": "^4.0.3",
    "pinia": "^3.0.3",
    "sass": "^1.90.0",
    "serve": "^14.2.4",
    "simple-git-hooks": "^2.13.1",
    "star-markdown-css": "^0.5.3",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "unocss": "^66.4.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.5"
  },
  "resolutions": {
    "unplugin": "^2.3.6",
    "vite": "^7.1.2"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
